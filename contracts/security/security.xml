<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="group_work_experience" model="res.groups">
            <field name="name">Work Experience</field>
        </record>

        <record model="ir.rule" id="employee_access_work_experience">
            <field name="name">Employee Access Work Experience</field>
            <field name="model_id" ref="model_work_experiences"/>
            <field name="domain_force">[('create_uid', '=', [user.id])]</field>
            <field name="perm_read" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_unlink" eval="True"/>
            <field name="perm_write" eval="False"/>
        </record>
    </data>
</odoo>